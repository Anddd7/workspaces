{
    "[csharp]": {
        "editor.defaultFormatter": "ms-dotnettools.csharp"
    },
    "[markdown]": {
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "[python]": {
        "editor.formatOnType": true
    },
    "[vue]": {
        "editor.defaultFormatter": "dbaeumer.vscode-eslint"
    },
    "Codegeex.Privacy": true,
    "Codegeex.Survey": false,
    "dart.onlyAnalyzeProjectsWithOpenFiles": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "editor.fontFamily": "Jetbrains Mono, Consolas, 'Courier New', monospace",
    "editor.formatOnSave": true,
    "editor.inlineSuggest.enabled": true,
    "eslint.format.enable": true,
    "excalidraw.theme": "auto",
    "files.autoSave": "afterDelay",
    "git.openRepositoryInParentFolders": "always",
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": true,
        "yaml": false
    },
    "gitlens.views.commits.avatars": false,
    "go.goroot": "/Users/adliao/.gvm/gos/go1.20",
    "redhat.telemetry.enabled": true,
    "terminal.external.osxExec": "iTerm.app",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "terminal.integrated.env.osx": {
        "FIG_NEW_SESSION": "1"
    },
    "terminal.integrated.fontFamily": "Jetbrains Mono, Consolas, 'Courier New', monospace",
    "terminal.integrated.lineHeight": 1.3,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Default Dark+ Experimental",
    "workbench.iconTheme": "vscode-icons",
    "workbench.startupEditor": "none",
    "yaml.format.singleQuote": true
}